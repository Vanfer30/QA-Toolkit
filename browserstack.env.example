# BrowserStack Configuration
# Get these from your BrowserStack account: https://www.browserstack.com/accounts/settings/access-keys

BROWSERSTACK_USERNAME=your_browserstack_username
BROWSERSTACK_ACCESS_KEY=your_browserstack_access_key

# Optional: BrowserStack Local specific settings
BROWSERSTACK_LOCAL_IDENTIFIER=qa-toolkit-local
BROWSERSTACK_LOCAL_VERBOSE=true

# Optional: Cypress specific settings
CYPRESS_BASE_URL=http://localhost:3000
CYPRESS_VIDEO_COMPRESSION=32
CYPRESS_RETRIES=2

