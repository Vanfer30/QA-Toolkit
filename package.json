  "scripts": {
    "test": "jest --config jest.config.ts",
    "test:unit": "jest --config jest.config.ts --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run --spec cypress/e2e/**/*.cy.{js,jsx,ts,tsx}",
    "test:features": "cypress run --spec cypress/e2e/features/**/*.feature",
    "test:login": "cypress run --spec cypress/e2e/features/login.feature",
    "test:user-management": "cypress run --spec cypress/e2e/features/user-management.feature",
    "test:search": "cypress run --spec cypress/e2e/features/search.feature",
    "test:registration": "cypress run --spec cypress/e2e/features/registration.feature",
    "test:component": "cypress run --testing-type component",
    "test:all": "npm run test:unit && npm run test:e2e && npm run test:component"
  }
}
